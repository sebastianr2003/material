def suma(a,b): #def función 
#suma de dos float o int
 resultado = a + b # suma de los dos dados a una 3 variable
 volver resultado # retirada el resultado de la función
#Si ejecutas el script directamente, `__name__` será `"__main__"`, por lo que el código dentro del bloque `if` se ejecutará.
if __name__ =="__main__":# llama al metodo suma
 num = suma(2,5)
 print(num)# print es escribir
